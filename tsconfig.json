{
  //   "include": ["src/*.ts"],
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist/js",
    "noEmitOnError": true,
    "moduleDetection": "force",
    "typeRoots": ["node_modules/@types"]
  },
  "exclude": ["node_modules"]
}
